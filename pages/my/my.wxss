/* pages/my/my.wxss */
.p1{
  position: relative;
  width: 100%;
  height: 460rpx;
  text-align: center;
  background-color: #fff;
}
.bg{
  position: absolute;
  z-index: 5;
  left: 0;
  top: 0;
  width: 100%;
  height: 460rpx;
}
.h{
  position: relative;
  z-index: 6;
  width: 175rpx;
  height: 175rpx;
  border-radius: 50%;
  margin:50rpx auto 0 auto;
}
.n{
  position: relative;
  z-index: 6;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  height: 90rpx;
  line-height: 90rpx;
  margin-bottom: 10rpx;
}
.btn_w{
  position: relative;
  z-index: 6;
}
.btn{
  font-size: 28rpx;
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: 0 auto;
  color: #353535;
}
.btn text{
  color: #999;
  font-weight: bold;
  font-size: 40rpx;
}

.navigator-hover {
    background:none;
    opacity: 0.9;
}
.p2{
  display: flex;
  flex-direction: row;
  background: #fff;
}
.p2 navigator{
  flex: 1;
  height: 170rpx;
  text-align: center;
}
.p2 image{
  width: 52rpx;
  height: 45rpx;
  display: block;
  margin: 50rpx auto 0;
}
.p2 text{
  color: #666;
  line-height: 75rpx;
  font-size: 28rpx;
}
.n2{
  border-left: 1rpx solid #e5e5e5;
  border-right: 1rpx solid #e5e5e5;
}

.p3{
  background: #fff;
  margin-top: 30rpx;
  padding: 0 30rpx;
}
.p3 navigator{
  display: block;
  position: relative;
  height: 100rpx;
  font-size: 30rpx;
  color: #353535;
  border-bottom: 1rpx solid #e5e5e5;
  line-height: 100rpx;
}
.p3 navigator:last-child{
  border-bottom: none;
}
.p3 image{
  width:15rpx;
  height: 25rpx;
  position: absolute;
  top: 37rpx;
  right: 0;
}