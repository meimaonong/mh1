<!--pages/my/my.wxml-->
<view class="container">

<view class="p1">
<image src="http://svn.meimaonong.com/mybg.jpg" class="bg"/>
<image src="{{userInfo.avatarUrl}}" class="h"/>
<view class="n">{{userInfo.nickName}}</view>
<navigator url="/pages/work_edit/work_edit" class="btn_w">
<button type="default" class="btn" hover-class="other-button-hover">
<text>+</text> 发布盘口 </button>
</navigator>
</view>

<view class="p2">
<navigator url="/pages/work_type1/work_type1">
<image src="http://svn.meimaonong.com/m1.png"/>
<text>待审溶剂<block wx:if="{{wait_num}}">({{wait_num}})</block></text>
</navigator>
<navigator url="/pages/album/album" class="n2">
<image src="http://svn.meimaonong.com/m2.png"/>
<text>我的盘口<block wx:if="{{album_num}}">({{album_num}})</block></text>
</navigator>
<navigator url="/pages/message/message">
<image src="http://svn.meimaonong.com/m3.png"/>
<text>我的消息</text>
<block wx:if="{{msg_num}}">
<view wx:if="{{msg_num>=10}}">...</view>
<view wx:if="{{msg_num<10}}">{{msg_num}}</view>
</block>
</navigator>
</view>

<view class="p3">
<navigator url="/pages/work_type2/work_type2">
<text>已出售溶剂<block wx:if="{{sell_num}}">({{sell_num}})</block></text>
<image src="http://svn.meimaonong.com/arrow.png"/>
</navigator>
<navigator url="/pages/work_type3/work_type3">
<text>已购买溶剂<block wx:if="{{buy_num}}">({{buy_num}})</block></text>
<image src="http://svn.meimaonong.com/arrow.png"/>
</navigator>
<navigator url="/pages/address/address">
<text>收货地址管理</text>
<image src="http://svn.meimaonong.com/arrow.png"/>
</navigator>
<navigator url="/pages/info/info">
<text>个人资料管理</text>
<image src="http://svn.meimaonong.com/arrow.png"/>
</navigator>
</view>

</view>
